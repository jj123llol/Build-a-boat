    [3] = workspace[plrteam()],
    [4] = CFrame.new(27, 8.100000381469727, 74) * CFrame.Angles(-0, 0, -0),
    [5] = true,
    [6] = 1,
    [7] = CFrame.new(-26.565689086914062, -9.899991989135742, -271.5068664550781) * CFrame.Angles(-0, 0, -0),
